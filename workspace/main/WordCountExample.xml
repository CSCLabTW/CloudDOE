<?xml version="1.0" encoding="UTF-8"?>
<CloudDOE xmlns="http://clouddoe.iis.sinica.edu.tw/confXML">
	<program>
		<name>WordCount</name>
		<jarfile>hadoop-examples-*.jar</jarfile>
		<clsname>wordcount</clsname>
		<version>20140213</version>
		<author>Hadoop Team</author>
		<lastupd>2014/02/14</lastupd>
		<website>http://wiki.apache.org/hadoop/WordCount</website>
		<argformat>$input $output &gt;&amp; $work/wordcount.log</argformat>
	</program>
	<parameters>
		<parameter>
			<label>Input</label>
			<arg></arg>
			<value>README</value>
			<type>input</type>
			<editable>true</editable>
		</parameter>
		<parameter>
			<label>Output</label>
			<arg></arg>
			<value>/Wordcount</value>
			<type>output</type>
			<editable>false</editable>
		</parameter>
		<parameter>
			<label>Work</label>
			<arg></arg>
			<value></value>
			<type>work</type>
			<editable>false</editable>
		</parameter>
	</parameters>
	<logs>
		<log>
			<name>wordcount.log</name>
			<type>detail</type>
		</log>
	</logs>
	<downloads>
		<download>
			<src>/Wordcount</src>
			<dst>result</dst>
			<merge>true</merge>
		</download>
	</downloads>
</CloudDOE>
