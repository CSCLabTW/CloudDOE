<?xml version="1.0" encoding="UTF-8"?>
<CloudDOE xmlns="http://clouddoe.iis.sinica.edu.tw/confXML">
	<program>
		<name>CloudBrush</name>
		<jarfile>CloudBrush.jar</jarfile>
		<lastupd>2013/10/01</lastupd>
		<helpurl>https://github.com/ice91/CloudBrush</helpurl>
		<argformat>$input $output $work $value $value</argformat>
	</program>
	<parameters>
		<parameter>
			<label>Input</label>
			<arg>-reads</arg>
			<value>/</value>
			<type>input</type>
			<editable>false</editable>
		</parameter>
		<parameter>
			<label>Output</label>
			<arg>-asm</arg>
			<value>/CloudBrush</value>
			<type>output</type>
			<editable>false</editable>
		</parameter>
		<parameter>
			<label>Work</label>
			<arg>-work</arg>
			<value>/</value>
			<type>work</type>
			<editable>false</editable>
		</parameter>
		<parameter>
			<label>Read length</label>
			<arg>-readlen</arg>
			<value>101</value>
			<type>value</type>
			<editable>true</editable>
		</parameter>
		<parameter>
			<label>Minimum overlap length</label>
			<arg>-k</arg>
			<value>25</value>
			<type>value</type>
			<editable>true</editable>
		</parameter>
	</parameters>
	<logs>
		<log>
			<name>brush.log</name>
			<type>short</type>
		</log>
		<log>
			<name>brush.details.log</name>
			<type>detail</type>
		</log>
	</logs>
	<downloads>
		<download>
			<method>get</method>
			<src>/CloudBrush.jpeg/stats.jpg</src>
			<dst>stats.jpg</dst>
		</download>
		<download>
			<method>getmerge</method>
			<src>/CloudBrush</src>
			<dst>output.fasta</dst>
		</download>
	</downloads>
</CloudDOE>
