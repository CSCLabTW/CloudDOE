<?xml version="1.0" encoding="UTF-8"?>
<CloudDOE xmlns="http://clouddoe.iis.sinica.edu.tw/confXML">
	<program>
		<name>CloudBurst</name>
		<jarfile>CloudBurst.jar</jarfile>
		<version>1.1.0</version>
		<author>Michael Schatz</author>
		<lastupd>2014/02/11</lastupd>
		<website>http://sourceforge.net/projects/cloudburst-bio/</website>
		<argformat>$input $input $output $value &gt;&amp; $work/burst.log</argformat>
	</program>
	<parameters>
		<parameter>
			<label>Reference</label>
			<arg></arg>
			<value>ref.br</value>
			<type>input</type>
			<editable>true</editable>
		</parameter>
		<parameter>
			<label>Query</label>
			<arg></arg>
			<value>qry.br</value>
			<type>input</type>
			<editable>true</editable>
		</parameter>
		<parameter>
			<label>Output</label>
			<arg></arg>
			<value>/CloudBurst</value>
			<type>output</type>
			<editable>false</editable>
		</parameter>
		<parameter>
			<label>Work</label>
			<arg></arg>
			<value></value>
			<type>work</type>
			<editable>false</editable>
		</parameter>
		<parameter>
			<label>Arguments</label>
			<arg></arg>
			<value>36 36 3 0 1 240 48 24 24 128 16</value>
			<type>value</type>
			<editable>true</editable>
		</parameter>
	</parameters>
	<logs>
		<log>
			<name>burst.log</name>
			<type>detail</type>
		</log>
	</logs>
	<downloads>
		<download>
			<src>/CloudBurst</src>
			<dst>results</dst>
		</download>
	</downloads>
</CloudDOE>
